.Bsizing{box-sizing: border-box;}
.wrap{width: 100%;height:auto;max-width: 750px;background: white;margin: 0 auto;}
.topbar{width: 100%;height:44px;line-height: 1rem;display: flex;position: fixed;top: 0;left: 0;z-index: 999;padding-right: 10px;
    padding-left: 10px;
    border-bottom: 0;
    background-color: #f7f7f7;
    -webkit-box-shadow: 0 0 1px rgba(0,0,0,.85);
    box-shadow: 0 0 1px rgba(0,0,0,.85);
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;}
.topbar-back,.topbar-next{width: 1.2rem;white-space: nowrap;font-size: .25rem;display: flex;align-content: center;justify-content: center;}
.topbar-back{width:1.2rem;}
.topbar-text{flex: 1;text-align: center;font-size: .35rem;}
.content{width: 100%;height: auto;margin-top:1rem ;}
.inform input[type='button']{background: #FB1647;border: 0;border-radius:.2rem ;padding: .01rem .2rem;color: #666666;height: .5rem;margin: .25rem auto;color: white;}
.userinform .input-group{display: flex;width: 7rem;margin: 0 auto;border-bottom: 1px solid gainsboro;height: 1rem;align-items: center;position: relative;}
.userinform .input-name{width: 1.8rem;}
.userinform .input-text{flex: 1;display: flex;justify-content: flex-end;align-items: center;}
.userinform .input-text input[type='file'],.userinform .input-text input[type='text']{width: 100%;height: .7rem;margin: 0;border: 0;text-align: right;padding: 0;}
.userinform .input-text input[type='file']{background: rgba(0,0,0,0);z-index: 10;position: absolute;top: 0;left: .8rem;opacity: 0;}
.userinform .myfile img{width: .8rem;height: .8rem;position: absolute;right: 0;top: 0;z-index: 1;}
.userinform .male{margin-right: .2rem;}