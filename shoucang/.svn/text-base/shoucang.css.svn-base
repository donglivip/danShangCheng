/*列表样式*/
#img_id img{width:100%}.mui-table-view:before{background-color:transparent}.itml_div{width:100%;float:right;background:#FFFFFF}.h4{width:69%;display:inline-block;margin-top:2px;max-height:45px;float:right;overflow:hidden}.h4 span{font-size:13px}.jiage p{padding:0;margin:8px 0 0 5px}.jiage{display:inline-block;margin-top:5px;font-size:20px;height:50px;line-height:20px;width:69%;float:right}.jiage p button{border:0;display:block;margin-top:-13px}.jiage p span{color:#FF0000;font-size:12px}.itml_div{padding:5px;border-bottom:1px solid #B6B6B6;overflow:hidden}.itml_div_l{width:30%;display:inline-block;height:100px;float:left}.itml_div_l img{width:100%;height:100%}.yanse{color:#FF0000}.kaituan{display:inline-block;background:#D83210;width:70px;height:40px;line-height:40px;text-align:center;margin-left:30px;color:#FFFFFF!important;font-size:20px;border-radius:5px}.jiage span{font-size:12px;padding:0 5px}