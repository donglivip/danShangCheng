mui.init();var input=document.getElementsByTagName('input');var button=document.getElementById("button");var address=document.getElementById("showCityPicker3");var title=document.getElementById("title");var aid=localStorage.getItem('aid');if(aid){title.innerHTML='修改收货地址'}else{title.innerHTML='添加收货地址'}input[0].value=localStorage.getItem('name');input[1].value=localStorage.getItem('phone');address.innerHTML=localStorage.getItem('addresss');input[2].value=localStorage.getItem('data_address');input[3].value=localStorage.getItem('zcode');button.addEventListener("tap",function(){var name=input[0].value;var phone=input[1].value;var addresss=address.innerHTML;var data_address=input[2].value;var zcode=input[3].value;if(name==''){mui.toast("收货人不能等于空");return false}if(phone==''){mui.toast("手机号码不能为空");return false}if(addresss==''){mui.toast("请选择收货地址");return false}if(data_address==''){mui.toast("详细地址不能为空");return false}if(zcode==''){mui.toast("邮编不能为空");return false}var register_url=localStorage.getItem("register_url");var login_phone=localStorage.getItem("login_phone");mui.get(register_url+'address',{name_data:name,phone_data:phone,addresss_data:addresss,data_address_data:data_address,zcode_data:zcode,login_phone_data:login_phone,aid_data:aid},function(data){if(data==1){mui.plusReady(function(){var home=plus.webview.getWebviewById('Home');mui.fire(home,'get_dizhi',{});mui.toast("提交成功");mui.back()})}else{mui.toast("提交失败，请重试！");return false}},'json')});